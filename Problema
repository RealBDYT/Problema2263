#include <iostream>

using namespace std;

int main()
{
    int t1;
    int t2;
    int n;
    int m;
    int z;

    cout<<"Numarul de camioane de tip 1??"<<endl;
    cin>>t1;

    if (t1 >= 2 and t1 <= 100) {

    cout<<"Numarul de camioane de tip 2??"<<endl;
    cin>>t2;

        if (t2 >= 2 and t2 <=100) {
    cout<<"Cate tone poate transporta camionul de tip1??"<<endl;
    cin>>m;

    cout<<"Cate tone poate transporta camionul de tip2??"<<endl;
    cin>>n;

    if (2 <= n and n <= 100 and 2<= m and m <= 100) {
    cout<<"In cate zile este transportata marfa??"<<endl;
    cin>>z;

    if (2 <= z <= 30) {
    cout<<"Numarul de tone transportate"<<endl;
    cout<<((t1 * m) + (t2 * n)) * z<<endl;
                } else {
                    cout<<"NU SE POATE"<<endl;
                    return 0;
    }
            } else {
                cout<<"NU SE POATE"<<endl;
                return 0;
    }
        } else {
        cout<<"NU SE POATE"<<endl;
        return 0;
    }
    } else {
    cout<<"NU SE POATE"<<endl;
    return 0;
    }

    return 0;
}
